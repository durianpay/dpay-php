{
    "name": "durianpay/dpay-php",
    "description": "SDK that wraps and collections of Durianpay's APIs, written in php",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "Durianpay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Durianpay\\": [
                "tests/"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
